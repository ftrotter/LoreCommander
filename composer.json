{
    "name": "ftrotter/lore",
    "description": "Lore Commander MTG Data Analysis Project",
    "keywords": [
        "MTG",
        "zermelo"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:CareSet/ZermeloBladeGraph.git"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "careset/zermelo_installer": "dev-master",
        "careset/zermelobladegraph": "dev-master",
        "fideloper/proxy": "~4.0",
        "ftrotter/two-table-monolog-mysql": "dev-master",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.0@dev",
        "phpunit/phpunit": "^7.3",
        "proai/laravel-handlebars": "^1.9",
        "pusher/pusher-php-server": "^4.0",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "superbalist/flysystem-google-storage": "^7.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "careset/durc": "dev-master",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
