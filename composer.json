{
    "name": "careset/lorecommander",
    "description": "Data analysis on MTG cards using the Zermelo reporting engine",
    "keywords": [
	"MTG",
	"Zermelo"
    ],
    "license": "Proprietary",
    "type": "project",
    "require": {
        "php": "^8.0",
        "careset/durc": ">=1.1.3",
        "careset/zermelo": ">=2.0",
        "davidgorges/color-contrast": "^1.0",
        "ftrotter/two-table-monolog-mysql": "dev-master",
        "jfcherng/php-diff": "^6.16",
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "phpunit/phpunit": "^7.3|^8.0",
        "proai/laravel-handlebars": "~1.9",
        "pusher/pusher-php-server": "^4.0",
        "superbalist/flysystem-google-storage": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
	    "CareSet\\": "CareSet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
