
<h1>salesreport list </h1>
<h3> <a href='/Zermelo/DURC_salesreport'>Zermelo salesreport Report</a></h3>
Create <a href='/DURC/salesreport/create/'>new salesreport</a><br>


{{^is_need_paging}}
<p>
{{total}} results. All rows shown.
</p>
{{/is_need_paging}}

{{#is_need_paging}}

<div class='dataTables_paginate paging_simple_numbers' id='table_salesreport_page'>
	<ul class='pagination'>
		<li class='paginate_button page-item previous {{first_page_class}}' 
			id='table_salesreport_previous'>
			<a href='{{first_page_url}}' aria-controls='table_salesreport' data-dt-idx='0' tabindex='0' class='page-link'>First</a>
		</li>
		<li class='paginate_button page-item {{prev_page_class}}'>
			<a href='{{prev_page_url}}' aria-controls='table_salesreport' data-dt-idx='1' tabindex='0' class='page-link'>Previous</a>
		</li>
		<li class='paginate_button page-item active'>
			<a href='#' aria-controls='table_salesreport' data-dt-idx='2' tabindex='0' class='page-link'>{{current_page}}</a>
		</li>
		<li class='paginate_button page-item {{next_page_class}} '>
			<a href='{{next_page_url}}' aria-controls='table_salesreport' data-dt-idx='3' tabindex='0' class='page-link'>Next</a>
		</li>
		<li class='paginate_button page-item {{last_page_class}}'>
			<a href='{{last_page_url}}' aria-controls='table_salesreport' data-dt-idx='4' tabindex='0' class='page-link'>Last</a>
		</li>
	</ul>
</div>
{{/is_need_paging}}


<table id='table_salesreport' class='table table-bordered table-hover table-responsive table-sm'>
<thead><tr>
<th scope='col'>groupBy</th>
<th scope='col'>display</th>
<th scope='col'>title</th>
<th scope='col'>filterRowSource</th>
<th scope='col'>default</th>

</tr></thead>
<tfoot><tr>
<th scope='col'>groupBy</th>
<th scope='col'>display</th>
<th scope='col'>title</th>
<th scope='col'>filterRowSource</th>
<th scope='col'>default</th>

</tr></tfoot>
<tbody>
  {{#data}}
    <tr>
				<td>{{groupBy}}</td>
			<td>{{display}}</td>
			<td>{{title}}</td>
			<td>{{filterRowSource}}</td>
			<td>{{default}}</td>

    </tr>
  {{/data}}

<tbody></table>



{{^is_need_paging}}
<p>
{{total}} results. All rows shown.
</p>
{{/is_need_paging}}

{{#is_need_paging}}

<div class='dataTables_paginate paging_simple_numbers' id='table_salesreport_page'>
	<ul class='pagination'>
		<li class='paginate_button page-item previous {{first_page_class}}' 
			id='table_salesreport_previous'>
			<a href='{{first_page_url}}' aria-controls='table_salesreport' data-dt-idx='0' tabindex='0' class='page-link'>First</a>
		</li>
		<li class='paginate_button page-item {{prev_page_class}}'>
			<a href='{{prev_page_url}}' aria-controls='table_salesreport' data-dt-idx='1' tabindex='0' class='page-link'>Previous</a>
		</li>
		<li class='paginate_button page-item active'>
			<a href='#' aria-controls='table_salesreport' data-dt-idx='2' tabindex='0' class='page-link'>{{current_page}}</a>
		</li>
		<li class='paginate_button page-item {{next_page_class}} '>
			<a href='{{next_page_url}}' aria-controls='table_salesreport' data-dt-idx='3' tabindex='0' class='page-link'>Next</a>
		</li>
		<li class='paginate_button page-item {{last_page_class}}'>
			<a href='{{last_page_url}}' aria-controls='table_salesreport' data-dt-idx='4' tabindex='0' class='page-link'>Last</a>
		</li>
	</ul>
</div>
{{/is_need_paging}}


<script type='text/javascript'>
//we need a way to check to see if the page is loaded before we call DataTable()
//but we frequently do not have JQuery yet... it could be loaded at the bottom of the page...
//so we have a pure JS alternative to the ready() function..
var DURC_checkReadyState = setInterval(() => {
  if (document.readyState === 'complete') {
    clearInterval(DURC_checkReadyState);
    // document is ready
    $('#table_salesreport').DataTable(
{
        'paging':   false,
        'info':     false
}
);
  }
}, 100);


</script>

